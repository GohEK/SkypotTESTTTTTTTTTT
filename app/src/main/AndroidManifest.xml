<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Skypot.app">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="com.miui.whetstone.permission.ACCESS_PROVIDER" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name="com.Skypot.app.chche.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.Skypot.app.MyCollectionActivity"></activity>
        <activity android:name="com.Skypot.app.AboutAppActivity" />
        <activity android:name="com.Skypot.app.CommodityTypeActivity" />
        <activity android:name="com.Skypot.app.ReviewCommodityActivity" />
        <activity android:name="com.Skypot.app.MyCommodityActivity" />
        <activity android:name="com.Skypot.app.ShoppingActivity" />
        <activity android:name="com.Skypot.app.ModifyInfoActivity" />
        <activity android:name="com.Skypot.app.MyInfoActivity" />
        <activity android:name="com.Skypot.app.ModifyPwdActivity" />
        <activity android:name="com.Skypot.app.PersonalCenterActivity" />
        <activity android:name="com.Skypot.app.AddCommodityActivity" />
        <activity android:name="com.Skypot.app.MainActivity" />
        <activity android:name="com.Skypot.app.RegisterActivity" />
        <activity android:name="com.Skypot.app.MyOrderActivity" />
        <activity android:name="com.Skypot.app.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>